<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:App.ViewModels;assembly=App"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="App.Views.MainWindow"
        FontFamily="/Fonts/Roboto-Regular.ttf#Roboto"      
        FontWeight="Regular"
        FontStyle="Normal"
        Background="{DynamicResource MainWindowColor}"
        BorderBrush="{DynamicResource MainWindowColor}"
        Height="600"
        Icon="avares://ugcs-geotagger/Assets/ugcs-logo-32.ico"
        Title="UgCS Geotagger">
  <UserControl  Content="{Binding Content}"></UserControl>
</Window>
